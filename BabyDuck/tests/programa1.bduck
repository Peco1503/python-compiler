program Factorial;
var
  n, factorial, i: int;

void imprimirFactorial();
{
    print("El factorial de " + n + " es: " + factorial);
}

main 
{
  n = 5;
  factorial = 1;
  i = 1;
  
  while (i <= n) do {
    factorial = factorial * i;
    i = i + 1;
  };
  
  if (factorial > 100) {
    print("El resultado es mayor que 100");
  } else {
    print("El resultado es menor o igual que 100");
  };
  
  imprimirFactorial();
}
end